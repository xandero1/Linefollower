
const int LedBlauw = 12;

void setup() {
  Serial.begin(9600);
  pinMode(LED_BUILTIN, OUTPUT);
  pinMode(LedBlauw, OUTPUT);
}

void loop() {
  digitalWrite(LED_BUILTIN, HIGH);   // LED in arduino nano aan
  digitalWrite(LedBlauw, HIGH);      // LED op breadboard aan
  delay(1000);
  digitalWrite(LED_BUILTIN, LOW);    // LED in arduino nano uit
  digitalWrite(LedBlauw, LOW);
  delay(1000);
}
